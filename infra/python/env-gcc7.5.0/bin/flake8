#!/home/noemi/impala/infra/python/env-gcc7.5.0/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'flake8==2.6.2','console_scripts','flake8'
__requires__ = 'flake8==2.6.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('flake8==2.6.2', 'console_scripts', 'flake8')()
    )
